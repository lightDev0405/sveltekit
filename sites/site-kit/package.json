	"devDependencies": {
		"@sveltejs/kit": "workspace:*",
		"@sveltejs/package": "workspace:*",
		"svelte": "^3.53.1",
		"typescript": "^4.9.3",
		"vite": "^3.2.4"
	},
	"dependencies": {
		"golden-fleece": "^1.0.9"
